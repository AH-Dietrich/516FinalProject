BR main 
a: .EQUATE 21
c: .EQUATE 3
m: .EQUATE 100
j: .WORD 0
temp: .WORD 0
seed: .WORD 0
rn: .WORD 0 
firstNum: .WORD 0
colcnt: .WORD 0
cmp: .WORD 0

main: DECI seed,d
      LDWA seed,d
      STWA rn,d
again: CALL product
      ADDA c,i
      STWA firstNum,d
      CALL mod
      CALL output
      LDWA cmp,d 
      CPWA 100,i
      BRGE end
      ADDA 1,i
      STWA cmp,d
      BR again
      


output: LDWX colcnt, d
        CPWX 10,i
        BRGE nline
cont:   DECO rn,d
        ADDX 1,i
        STWX colcnt,d
        LDBX ' ',i
        STBX charOut,d
        RET
nline:  LDBX '\n',i
        STBX charOut,d
        LDWX 0,i
        STWX colcnt,d
        BR cont
        


mod:  LDWX 0,i
      LDWA firstNum,d
top:  SUBA m,i
      ADDX 1,i
      CPWA m,i 
      BRLT finish
      BR top
finish: STWA rn,d
      RET


product:  LDWX 0,i
          STWX j,d
          LDWA 0,i
for1:     CPWX rn,d
          BRGE endFor1
          ADDA a,i
          LDWX j,d
          ADDX 1,i
          STWX j,d
          BR for1
endFor1:  STWA temp,d
          RET

end: .END
